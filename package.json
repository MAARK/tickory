{
    "name": "tickory",
    "version": "0.8.0",
    "license": "MIT",
    "author": "Rich Wagner, Maark",
    "description": "CLI tool to enter time using Tickspot.",
    "keywords": [
        "tick",
        "time tracking",
        "cli",
        "tickspot",
        "time entry"
    ],
    "main": "index.js",
    "preferGlobal": true,
    "bin": {
        "tickory": "bin/tickory"
    },
    "scripts": {
        "test": "jshint . --exclude node_modules"
    },
    "jshintConfig": {
        "undef": true,
        "unused": true,
        "esversion": 9,
        "node": true,
        "globals": {
            "$": true
        }
    },
    "devDependencies": {
        "cpass": "^2.1.1"
    },
    "dependencies": {
        "bluebird": "^3.5.3",
        "chalk": "^2.4.2",
        "cli-spinner": "^0.2.10",
        "colors": "^1.3.3",
        "fs": "0.0.1-security",
        "inquirer": "^6.3.1",
        "inquirer-datepicker-prompt": "^0.4.2",
        "jshint": "^2.10.2",
        "lodash": "^4.17.11",
        "mkdirp": "^0.5.1",
        "moment": "^2.24.0",
        "prompts": "^2.1.0",
        "table": "^5.4.0",
        "tcomb-validation": "^3.4.1",
        "tickspotv2-api": "^1.1.0",
        "yargs": "^13.2.2"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/MAARK/tickory.git"
    },
    "bugs": {
        "url": "https://github.com/MAARK/tickory/issues"
    }
