        "cpass": "^2.2.0",
        "fs": "^0.0.2",
        "inquirer": "^7.0.6",
        "inquirer-datepicker-prompt": "^0.4.2",
        "jshint": "^2.11.0",
        "lodash": "^4.17.15",
        "mkdirp": "^1.0.3",
        "moment": "^2.24.0",
        "prompts": "^2.3.1",
        "table": "^5.4.6",
        "tcomb-validation": "^3.4.1",
        "tickspotv2-api": "^1.1.0",
        "yargs": "^15.1.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/MAARK/tickory.git"
